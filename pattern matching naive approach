****************************************************************************************
input  : s -> abcdef
         p -> bcd
output : true;

input  : s -> abcgfh
         p -> klp
output : false
****************************************************************************************

#include<bits/stdc++.h>
using namespace std;

bool matching_exist(string s, string p)
{
    int n = s.length(), m = p.length();
    for(int i = 0; i <= (n-m); i++)
    {
         bool isfound = true;
        for(int j = 0; j < m; j++)
        {
            if(s[i+j] != p[j])
            {
                isfound = false;
                break;
            }
        }
        if(isfound == true)
        return true;
    }
    return false;
}
int main()
{
    string s, p;
    cin >> s >> p;
    cout << matching_exist(s,p);
    return 0;
}
